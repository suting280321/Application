<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <welcome-file-list>
    <welcome-file>index3.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>404</error-code>
    <location>/errorPage.jsp</location>
  </error-page>
  <display-name>应用案例</display-name>
  <servlet>
    <servlet-name>ReadFile_v1</servlet-name>
    <servlet-class>cn.test.spare.ReadFile_v1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>readContent</servlet-name>
    <servlet-class>cn.test.spare.readContent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReadFile_v1</servlet-name>
    <url-pattern>/ReadFile1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>readContent</servlet-name>
    <url-pattern>/readContent1</url-pattern>
  </servlet-mapping>
</web-app>